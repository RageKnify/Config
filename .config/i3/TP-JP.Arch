# i3 config for Thinkpad

# Assign Workspaces:
assign [class="chrome"] $ws1
assign [class="Ghidra"] $ws3
assign [class="discord"] $ws5
assign [class="Gimp"] $ws6

exec --no-startup-id compton -CGb
exec --no-startup-id /home/jp/.fehbg
exec --no-startup-id redshift
exec --no-startup-id xautolock -notify 2 -notifier /bin/notify-send -time 10 -locker '/home/jp/bin/lock.sh'
